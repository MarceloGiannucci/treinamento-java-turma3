package br.com.treinamento;

public class Conta {
	//Atributos
	String agencia;
	int numeroConta;
	double saldo;
	
	void transferir( ) {
		
	}
	
	void sacar() {
		
	}
	
	void depositar(double valor) { // assinatura o que determina que é
